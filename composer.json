{
    "name": "denpa/laravel-bitcoinrpc",
    "type": "library",
    "description": "Bitcoin JSON-RPC Service Provider for Laravel",
    "keywords": ["laravel","bitcoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/denpamusic/php-bitcoinrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Paavilainen",
            "email": "denpa@netfleet.space",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "denpa/php-bitcoinrpc": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "orchestra/testbench": "3.x"
    },
    "autoload": {
        "psr-4" : {
            "Denpa\\Bitcoin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denpa\\Bitcoin\\Providers\\ServiceProvider"
            ]
        }
    }
}
